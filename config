DefaultErrorPage                    ./www/Error.html
DefaultRoot                         ./www/Default
DefaultUploadDir                    ./www/upload
DefaultIndex                        ./www/Default/index.html
DefaultMaxClientBody                16000

Server {
    Listen              44
    ServerName          default
    ErrorPage           400 401 402 ./www/Error.html
    Host                gg

    Location / {
        Root            ./www/Default
        Index           index.html
        AutoIndex       on
        Methods         GET POST
        Redir           /new-page
    }

    Location /cgi {
        IsCgi           on
    }
}

Server {
    Listen 33
    ServerName HttpServer
    Host  0.0.0.0

    Location / {
        Root ./www/Default
        Index index.html
        AutoIndex       on
        Methods         GET POST PUT
    }
}
