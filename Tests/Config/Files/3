DefaultErrorPage                    ./www/Error.html
DefaultRoot                         ./www/Default
DefaultUploadDir                    ./www/upload
DefaultIndex                        ./www/Default/index.html
DefaultMaxClients                   900
DefaultMaxClientBody                16000


Server {
    Listen 80
    ServerName www.example.com


    Location / {
        Index index.html
        AutoIndex on
        Methods GET POST
        Redir /new-page
        Root /Default
    }

