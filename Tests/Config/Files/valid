DefaultErrorPage     ./www/Error.html

#need unique ServerNames

Server {
    Listen 80
    ServerName www.example.com
    MaxClients          100
    ErrorPage     400 401 402 ./www/Default/ServerError.html
    Host         gg

    Location / {
        Root /www/Default
        Index index.html
        AutoIndex on
        Methods GET POST
        Redir /new-page
        ClientMaxBodySize   16000
    }

    Location / {
        Root /www/Default
        Index index.html
        AutoIndex on
        Methods GET POST
        Redir /new-page
    }
}

Server {
    Listen 80
    ServerName www.examle.com
    MaxClients          100
    ErrorPage     400 401 402 ./www/Default/ServerError.html
    Host         gg
}
