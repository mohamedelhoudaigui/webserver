

http {
    error_log /logs/error.log;
    access_log /logs/access.log;

    root /;
    server {
        root /hhh;
        error_page 500  /errors/index.hmtl;
        listen 90;
        server_name localhost;
        location {
            root /www;
            index index.html;
            autoindex on|off;

            return 404 "https:/...";

            cgi_pass /wwww/index.php;

            methods GET, PUT, POST, DELETE;
        }
    }
}